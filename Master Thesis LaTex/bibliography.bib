@article{hachmannShorttermMemoryOrder2014,
  title = {Short-Term Memory for Order but Not for Item Information Is Impaired in Developmental Dyslexia},
  author = {Hachmann, Wibke M. and Bogaerts, Louisa and Szmalec, Arnaud and Woumans, Evy and Duyck, Wouter and Job, Remo},
  date = {2014-07-01},
  journaltitle = {Annals of Dyslexia},
