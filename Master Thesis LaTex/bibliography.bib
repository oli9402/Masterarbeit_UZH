@article{gadeynePsychosocialFunctioningYoung2004,
  title = {Psychosocial Functioning of Young Children with Learning Problems},
  author = {Gadeyne, Els and Ghesquière, Pol and Onghena, Patrick},
  date = {2004},
  journaltitle = {Journal of Child Psychology and Psychiatry},
  volume = {45},
  number = {3},
