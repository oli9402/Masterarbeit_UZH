@article{williamsERPFrequencyTime2021,
  title = {The {{ERP}}, Frequency, and Time–Frequency Correlates of Feedback Processing: {{Insights}} from a Large Sample Study},
  shorttitle = {The {{ERP}}, Frequency, and Time–Frequency Correlates of Feedback Processing},
  author = {Williams, Chad C. and Ferguson, Thomas D. and Hassall, Cameron D. and Abimbola, Wande and Krigolson, Olave E.},
  date = {2021},
