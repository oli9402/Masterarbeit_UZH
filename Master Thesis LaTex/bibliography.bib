@article{kannenP300ModulationTranscranial2022,
  title = {P300 {{Modulation}} via {{Transcranial Alternating Current Stimulation}} in {{Adult Attention-Deficit}}/{{Hyperactivity Disorder}}: {{A Crossover Study}}},
  shorttitle = {P300 {{Modulation}} via {{Transcranial Alternating Current Stimulation}} in {{Adult Attention-Deficit}}/{{Hyperactivity Disorder}}},
  author = {Kannen, Kyra and Aslan, Behrem and Boetzel, Cindy and Herrmann, Christoph S. and Lux, Silke and Rosen, Helena and Selaskowski, Benjamin and Wiebe, Annika and Philipsen, Alexandra and Braun, Niclas},
  date = {2022},
