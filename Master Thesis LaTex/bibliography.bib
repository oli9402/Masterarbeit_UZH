@article{sommerMemorySpecificityLinked2021,
  title = {Memory Specificity Is Linked to Repetition Effects in Event-Related Potentials across the Lifespan},
  author = {Sommer, Verena R. and Mount, Luzie and Weigelt, Sarah and Werkle-Bergner, Markus and Sander, Myriam C.},
  date = {2021-04-01},
  journaltitle = {Developmental Cognitive Neuroscience},
