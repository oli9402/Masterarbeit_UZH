@article{wangDysregulationProfileDP2022,
  title = {Dysregulation Profile ({{DP}}) as a Transdiagnostic Psychopathological Factor in Clinically Referred Children – Comparisons between Disorders and Latent Structure},
  author = {Wang, Biyao and Becker, Andreas and Kaelble, Christina and Rothenberger, Aribert and Uebel-von Sandersleben, Henrik},
  date = {2022-01-02},
  journaltitle = {Nordic Journal of Psychiatry},
