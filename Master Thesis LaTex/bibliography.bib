@article{chenEffectsDialecticalBehaviour2021,
  title = {Effects of Dialectical Behaviour Therapy on Reducing Self-Harming Behaviours and Negative Emotions in Patients with Borderline Personality Disorder: {{A}} Meta-Analysis},
  shorttitle = {Effects of Dialectical Behaviour Therapy on Reducing Self-Harming Behaviours and Negative Emotions in Patients with Borderline Personality Disorder},
  author = {Chen, Shu-Yan and Cheng, Yin and Zhao, Wei-Wei and Zhang, Yan-Hong},
  date = {2021},
  journaltitle = {Journal of Psychiatric and Mental Health Nursing},
  volume = {28},
