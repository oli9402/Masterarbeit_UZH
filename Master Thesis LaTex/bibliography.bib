  author = {Manning, Joshua and Hedden, Trey and Wickens, Nina and Whitfield-Gabrieli, Susan and Prelec, Drazen and Gabrieli, John D. E.},
  date = {2014-09-01},
  journaltitle = {NeuroImage},
