@article{haslbeckHowPredictableAre2017,
  title = {How Predictable Are Symptoms in Psychopathological Networks? {{A}} Reanalysis of 18 Published Datasets},
  shorttitle = {How Predictable Are Symptoms in Psychopathological Networks?},
  author = {Haslbeck, J. M. B. and Fried, E. I.},
