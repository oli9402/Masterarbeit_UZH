  author = {Miyazaki, Masahito and Shibasaki, Hiroshi and Suwazono, Shugo and Honda, Manabu and Ikeda, Akio and Nagamine, Takashi and Nishida, Shigeto and Nakamura, Masatoshi and Hayakawa, Takahiro and Mutoh, Kozo and Mikawa, Haruki},
