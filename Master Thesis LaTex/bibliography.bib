@article{jungeblutInvestigationSlopeParameters,
  title = {An {{Investigation}} of the {{Slope Parameters}} of {{Reaction Times}} and {{P3 Latencies}} in the {{Sternberg Memory Scanning Task}} – {{A Fixed-Links Model Approach}}},
  author = {Jungeblut, Henrike M. and Hagemann, Dirk and Löffler, Christoph and Schubert, Anna-Lena},
