@article{patronoTransitionalityAddictionTemporal2016,
  title = {Transitionality in Addiction: {{A}} “Temporal Continuum” Hypotheses Involving the Aberrant Motivation, the Hedonic Dysregulation, and the Aberrant Learning},
  shorttitle = {Transitionality in Addiction},
  author = {Patrono, Enrico and Gasbarri, Antonella and Tomaz, Carlos and Nishijo, Hisao},
  date = {2016-08-01},
  journaltitle = {Medical Hypotheses},
