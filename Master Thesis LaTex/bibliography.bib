  shorttitle = {The {{Effects}} of {{Aging}} and {{Time}} of {{Day}} on {{Inhibitory Control}}},
  author = {Rabi, Rahel and Chow, Ricky and Paracha, Shahier and Hasher, Lynn and Gardner, Sandra and Anderson, Nicole D. and Alain, Claude},
  date = {2022},
  journaltitle = {Frontiers in Aging Neuroscience},
  volume = {14},
