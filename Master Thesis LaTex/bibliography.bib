  author = {Guevara, Miguel Angel and Cruz Paniagua, Edwin Iván and Hernández González, Marisela and Sandoval Carrillo, Ivett Karina and Almanza Sepúlveda, Mayra Linné and Hevia Orozco, Jorge Carlos and Amezcua Gutiérrez, Claudia},
