  author = {Kleinstäuber, Maria and Allwang, Christine and Bailer, Josef and Berking, Matthias and Brünahl, Christian and Erkic, Maja and Gitzen, Harald and Gollwitzer, Mario and Gottschalk, Japhia-Marie and Heider, Jens and Hermann, Andrea and Lahmann, Claas and Löwe, Bernd and Martin, Alexandra and Rau, Jörn and Schröder, Annette and Schwabe, Johannes and Schwarz, Jeanine and Stark, Rudolf and Weiss, Frauke Dorothee and Rief, Winfried},
  date = {2019},
