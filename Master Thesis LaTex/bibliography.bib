@article{kwakoAddictionsNeuroclinicalAssessment2016,
  title = {🟢{{Addictions Neuroclinical Assessment}}: {{A Neuroscience-Based Framework}} for {{Addictive Disorders}}},
  shorttitle = {Addictions {{Neuroclinical Assessment}}},
  author = {Kwako, Laura E. and Momenan, Reza and Litten, Raye Z. and Koob, George F. and Goldman, David},
  date = {2016-08-01},
  journaltitle = {Biological Psychiatry},
  shortjournal = {Biological Psychiatry},
  series = {Insights {{Into Cocaine}} and {{Nicotine Addictions}}},
  volume = {80},
